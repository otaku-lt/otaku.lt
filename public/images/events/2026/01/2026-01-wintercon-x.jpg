<script>
(function () {
  const SDK_URL = 'https://store.bilesuserviss.lv/public/widgetApi/v2/sdk.min.js?id=22065';
  let loaded = false;

  function loadSDK(cb){ 
    if (loaded) { cb && cb(); return; }
    const s = document.createElement('script');
    s.src = SDK_URL;
    s.async = true;
    s.onload = function(){ loaded = true; cb && cb(); };
    document.body.appendChild(s);
  }

  // 1) Тихо загружаем при первом взаимодействии
  window.addEventListener('pointerdown', () => loadSDK(), {once:true, passive:true});
  window.addEventListener('keydown',     () => loadSDK(), {once:true});
  window.addEventListener('mousemove',   () => loadSDK(), {once:true, passive:true});
  window.addEventListener('touchstart',  () => loadSDK(), {once:true, passive:true});

  // 2) Или когда браузер простаивает / через таймер
  (window.requestIdleCallback || function(f){ setTimeout(f, 1500); })(loadSDK);

  // 3) На всякий случай: если пользователь всё-таки кликнул раньше — дозагрузим и повторим клик
  document.addEventListener('click', function(e){
    const btn = e.target.closest('[data-widgetpopup="true"]');
    if (!btn || loaded) return;
    e.preventDefault();
    btn.classList.add('is-loading'); // покажи спиннер на кнопке
    loadSDK(() => { btn.classList.remove('is-loading'); btn.click(); });
  }, {passive:false});
})();
</script>

<script>
var Webflow = Webflow || [];
Webflow.push(function () {
  var tabName = getParam('tab');
  if (!tabName) return;

  $('.' + tabName).triggerHandler('click');

  function getParam(name) {
    name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
      results = regex.exec(location.search);
    return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
  }
});
</script>

<script>
var Webflow = Webflow || [];
Webflow.push(function () {
  var tabName = getParam('tab');
  var targetID = location.hash; // например #guest-taeyeon

  if (tabName) {
    // 1. Открываем нужный таб
    $('.' + tabName).triggerHandler('click');

    // 2. Ждем пока Webflow переключит таб (около 50–200мс)
    setTimeout(function () {
      if (targetID) {
        var $el = $(targetID);
        if ($el.length) {
          // 3. Скроллим к нужному гостю
          $('html, body').animate({
            scrollTop: $el.offset().top - 80  // -80 = компенсация для отступа под хедер. Можно изменить.
          }, 500);
        }
      }
    }, 300); // задержка чтобы таб успел показаться
  }

  function getParam(name) {
    name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
      results = regex.exec(location.search);
    return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
  }
});
</script>
